{
    "name": "Taskaty",
    "manifest_version": 3,
    "version": "1.0",
    "description": "A simple to-do list maker",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
      }
    },
    "background": {
      "service_worker": "service.js"
    },
    "icons": {
      "16": "images/icon.png",
      "48": "images/icon.png",
      "128": "images/icon.png"
    },
    "permissions": ["storage"]
  }
  